            <% } %>
        </div>
        <div class = "Checkout Menu">

        </div>
    </div>

    <footer>
        <button class = "change-language-button">

        </button>

    </footer>

    <% for (var i in menu_items) {%>
        <p>Menu Item: <%=menu_items[i].name%>, price<%=menu_items[i].price%> </p>
    <% } %>
    <% for (var i in flavors) {%>
        <p>Flavor: <%=flavors[i].name%> </p>
    <% } %>

